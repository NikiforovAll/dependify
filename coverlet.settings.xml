<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura,opencover</Format>
          <Exclude>[*Tests]*,[*.Client]*,[EPAM.USPP.Testing.Shared]*,[EPAM.USPP.JobsApp.Migrations]*</Exclude> <!-- [Assembly-Filter]Type-Filter -->
          <ExcludeByFile>**/*.g.cs,</ExcludeByFile> <!-- Globbing filter -->
          <IncludeTestAssembly>false</IncludeTestAssembly>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>